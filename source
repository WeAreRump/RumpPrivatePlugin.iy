local Version = "0.1"

local Plugin = {
	["PluginName"] = "Rump's Private Plugin v" .. Version,
	["PluginDescription"] = "Fun plugin",

	["Commands"] = {
		["reload"] = {
			["ListName"] = "reload",
			["Description"] = "Rump's Private Plugin\nReloads IY.",
			["Aliases"] = {},
			["Function"] = function(args, speaker)
				game.CoreGui.Main:Destroy()
				wait()
				loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
			end
		},
	}
}

return Plugin
