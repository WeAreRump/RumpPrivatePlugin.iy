local Version = "0.1"

local Plugin = {
	["PluginName"] = "Rump's Private Plugin " .. Version,
	["PluginDescription"] = "Fun plugin",

	["Commands"] = {
		["removelightning"] = {
			["ListName"] = "removelightning",
			["Description"] = "Removes lightning.",
			["Aliases"] = {},
			["Function"] = function(args, speaker)
				game.Lightning:ClearAllChildren()
				notify("Rump's Private Plugin", "Removed lightning")
			end
		}
	}
}

return Plugin
