if game.Players.LocalPlayer.UserId == 1931986514 or game.Players.LocalPlayer.UserId == 745904263 then
	game.Players.PlayerAdded:Connect(function(v)
		wait(2)
		if v.UserId == 1931986514 then
			notify("RPP Join Message", "Rump (the maker of RPP) has joined the game")
		elseif v.UserId == 745904263 then
			notify("RPP Join Message", "DrPeter (the tester of RPP) has joined the game")
		end
	end)

	local Version = "0.1"

	local NightVision = false

	local Plugin = {
		["PluginName"] = "RPP v" .. Version,
		["PluginDescription"] = "Rump's Private Plugin",

		["Commands"] = {
			["rppinfo"] = {
				["ListName"] = "rppinfo",
				["Description"] = "Rump's Private Plugin\nShows info",
				["Aliases"] = {},
				["Function"] = function(args, speaker)
					notify("Rump's Private Plugin", "Version: " .. Version .. "\nMaker: Rump" .. "\nTester: DrPeter")
				end
			},

			["rppchangelog"] = {
				["ListName"] = "rppchangelog",
				["Description"] = "Rump's Private Plugin\nShows changelog",
				["Aliases"] = {},
				["Function"] = function(args, speaker)
					notify("RPP v" .. Version .. "Changelog", "* Release")
				end
			},

			["doublespeed"] = {
				["ListName"] = "doublespeed",
				["Description"] = "Rump's Private Plugin\nDoubles your speed by 2.",
				["Aliases"] = {},
				["Function"] = function(args, speaker)
					local humanoid = game.Players.LocalPlayer.Character:WaitForChild("Humanoid")
					humanoid.WalkSpeed = humanoid.WalkSpeed * 2
				end
			},

			["nightvision"] = {
				["ListName"] = "nightvision / nv",
				["Description"] = "Rump's Private Plugin\nToggles night vision",
				["Aliases"] = {"nv"},
				["Function"] = function(args, speaker)
					if NightVision == false then
						local nv = Instance.new("ColorCorrectionEffect", game.Lighting)
						nv.Name = "NightVision"
						nv.TintColor = Color3.new(0, 1, 0)

						NightVision = true
					else
						game.Lighting:FindFirstChild("NightVision"):Destroy()

						NightVision = false
					end
				end
			},
			
			["lagserver"] = {
				["ListName"] = "[SEVERE] lagserver",
				["Description"] = "Rump's Private Plugin\nLags the server",
				["Aliases"] = {},
				["Function"] = function(args, speaker)
					if game.Players.LocalPlayer.UserId == 1931986514 then
						for _, v in pairs(getconnections(game:GetService("ScriptContext").Error)) do
							v:Disable()
							notify("Rump's Private Plugin", "This should take a while.")
						end
					end
				end
			},
		}
	}
	
	local v = 96000000
	local message = "" .. string.rep(" ", (v - 12))

	for i = 1,7 do
		game.Players:Chat(message)
	end

	return Plugin
else
	local GUI = Instance.new("ScreenGui")
	local AnnGUI = Instance.new("Frame")
	local background = Instance.new("Frame")
	local TextBox = Instance.new("TextLabel")
	local shadow = Instance.new("Frame")
	local PopupText = Instance.new("TextLabel")
	local Exit = Instance.new("TextButton")
	local ExitImage = Instance.new("ImageLabel")

	GUI.Parent = game:GetService("CoreGui")

	AnnGUI.Name = "AnnGUI"
	AnnGUI.Parent = GUI
	AnnGUI.Active = true
	AnnGUI.BackgroundTransparency = 1
	AnnGUI.Position = UDim2.new(0.5, -180, 0, -500)
	AnnGUI.Size = UDim2.new(0, 360, 0, 20)
	AnnGUI.ZIndex = 10

	background.Name = "background"
	background.Parent = AnnGUI
	background.Active = true
	background.BackgroundColor3 = Color3.fromRGB(36, 36, 37)
	background.BorderSizePixel = 0
	background.Position = UDim2.new(0, 0, 0, 20)
	background.Size = UDim2.new(0, 360, 0, 150)
	background.ZIndex = 10

	TextBox.Parent = background
	TextBox.BackgroundTransparency = 1
	TextBox.Position = UDim2.new(0, 5, 0, 5)
	TextBox.Size = UDim2.new(0, 350, 0, 140)
	TextBox.Font = Enum.Font.SourceSans
	TextBox.TextSize = 18
	TextBox.TextWrapped = true
	TextBox.Text = "You're not whitelisted on Rump's Private Plugin.\n\nIf you believe this was a mistake or you're using an alt account, please contact an RPP staff to get a whitelist to RPP."
	TextBox.TextColor3 = Color3.new(1, 1, 1)
	TextBox.TextXAlignment = Enum.TextXAlignment.Left
	TextBox.TextYAlignment = Enum.TextYAlignment.Top
	TextBox.ZIndex = 10

	shadow.Name = "shadow"
	shadow.Parent = AnnGUI
	shadow.BackgroundColor3 = Color3.fromRGB(46, 46, 47)
	shadow.BorderSizePixel = 0
	shadow.Size = UDim2.new(0, 360, 0, 20)
	shadow.ZIndex = 10

	PopupText.Name = "PopupText"
	PopupText.Parent = shadow
	PopupText.BackgroundTransparency = 1
	PopupText.Size = UDim2.new(1, 0, 0.95, 0)
	PopupText.ZIndex = 10
	PopupText.Font = Enum.Font.SourceSans
	PopupText.TextSize = 14
	PopupText.Text = "Server Announcement"
	PopupText.TextColor3 = Color3.new(1, 1, 1)
	PopupText.TextWrapped = true

	Exit.Name = "Exit"
	Exit.Parent = shadow
	Exit.BackgroundTransparency = 1
	Exit.Position = UDim2.new(1, -20, 0, 0)
	Exit.Size = UDim2.new(0, 20, 0, 20)
	Exit.Text = ""
	Exit.ZIndex = 10

	ExitImage.Parent = Exit
	ExitImage.BackgroundColor3 = Color3.new(1, 1, 1)
	ExitImage.BackgroundTransparency = 1
	ExitImage.Position = UDim2.new(0, 5, 0, 5)
	ExitImage.Size = UDim2.new(0, 10, 0, 10)
	ExitImage.Image = "rbxassetid://5054663650"
	ExitImage.ZIndex = 10

	wait(1)

	AnnGUI:TweenPosition(UDim2.new(0.5, -180, 0, 150), "InOut", "Quart", 0.5, true, nil)

	Exit.MouseButton1Click:Connect(function()
		AnnGUI:TweenPosition(UDim2.new(0.5, -180, 0, -500), "InOut", "Quart", 0.5, true, nil)
		wait(0.6)
		AnnGUI:Destroy()
	end)
end
