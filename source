local Version = "0.1"

local Plugin = {
	["PluginName"] = "Rump's Private Plugin v" .. Version,
	["PluginDescription"] = "Fun plugin",

	["Commands"] = {
		["removelightning"] = {
			["ListName"] = "removelightning / rl",
			["Description"] = "Rump's Private Plugin\nRemoves lightning.",
			["Aliases"] = {"rl"},
			["Function"] = function(args, speaker)
				game.Lightning:ClearAllChildren()
				notify("Rump's Private Plugin", "Removed lightning")
			end
		},
	}
}

return Plugin
